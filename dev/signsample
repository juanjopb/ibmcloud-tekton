      %echo Generating a basic OpenPGP key
      Key-Type: DSA
      Key-Length: 1024
      Subkey-Type: ELG-E
      Subkey-Length: 1024
      Name-Real: SignSample
      Name-Email: signsample@foo.bar
      Expire-Date: 0
      Passphrase: abc
      %pubring signsample.pub
      %secring signsample.sec
      # Do a commit here, so that we can later print "done" :-)
      %commit
      %echo done
